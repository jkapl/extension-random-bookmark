{
  "name" : "Random Bookmark",
  "version": "1.0",
  "description": "Open a random bookmark",
  "background" : {
    "scripts": ["index.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions" : [
    "tabs", "storage", "bookmarks"
  ],
  "manifest_version": 2
}